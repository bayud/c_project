{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "C:/MinGW/include/*",
                "C:/MinGW/lib/gcc/mingw32/5.3.0/include",
                "${workspaceFolder}/package/header/*",
                "${workspaceFolder}/package/c/*"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "browse": {
                "path":[
                    "${workspaceFolder}/package/header",
                    "${workspaceFolder}/package/c"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "intelliSenseMode": "msvc-x64"
        }
    ],
    "version": 4
}